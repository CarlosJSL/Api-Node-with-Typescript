{
  "name": "api-node-with-typescript",
  "version": "1.0.0",
  "description": "Node api using typescript",
  "main": "./server/server.ts",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builder": "NODE_ENV=development node ./build/server/server.js",
    "unit-test": "NODE_ENV=test mocha --opts build/tests/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts build/tests/integration/config/mocha.opts ./build/tests/integration/integration.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CarlosJSL/Api-Node-with-Typescript.git"
  },
  "author": "Carlos Jose",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CarlosJSL/Api-Node-with-Typescript/issues"
  },
  "homepage": "https://github.com/CarlosJSL/Api-Node-with-Typescript#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "http": "0.0.0",
    "morgan": "^1.8.2",
    "pg": "^7.0.2",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.7.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.1",
    "@types/express": "^4.0.36",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.32",
    "@types/sequelize": "^4.0.67",
    "@types/supertest": "^2.0.2",
    "chai": "^4.1.0",
    "mocha": "^3.5.0",
    "sequelize": "^4.4.2",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  }
}
